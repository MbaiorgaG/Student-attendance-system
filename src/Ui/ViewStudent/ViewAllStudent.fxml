<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="10.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Ui.ViewStudent.ViewAllStudentController">
    <children>
        <HBox spacing="10.0" />
        <TableView fx:id="list_table" stylesheets="@../StyleCheet/style.css" VBox.vgrow="ALWAYS">
            <columns>
                <TableColumn fx:id="name_col" maxWidth="200.0" minWidth="100.0" prefWidth="179.0" text="STUDENT NAME" />
                <TableColumn fx:id="id_col" maxWidth="400.0" minWidth="170.0" prefWidth="170.0" text="STUDNENT MATRIC" />
                <TableColumn fx:id="course_col" maxWidth="141.0" minWidth="90.0" prefWidth="141.0" text="LEVEL" />
            </columns>
        </TableView>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</VBox>
